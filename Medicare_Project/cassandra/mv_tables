CREATE TABLE mv_providers_cost_v5(hcpcs_code text, nppes_provider_state text, year int, npi text, place_of_service text, nppes_provider_last_org_name text, nppes_provider_first_name text, nppes_provider_mi text, nppes_provider_zip text, average_submitted_chrg_amt float, stdev_submitted_chrg_amt float, PRIMARY KEY ((hcpcs_code, year, nppes_provider_state), average_submitted_chrg_amt, nppes_provider_last_org_name, npi, place_of_service)) WITH CLUSTERING ORDER BY (average_submitted_chrg_amt DESC, nppes_provider_last_org_name DESC, npi DESC, place_of_service ASC);



 
